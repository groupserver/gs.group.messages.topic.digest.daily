<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal">
/* Digest CSS */
.gs-group-messages-topic-digest-daily-topic td {
    padding: 0 0 20px 0;
}
td.gs-group-messages-topic-digest-daily-topic-info {
    padding: 0 0 20px 10px;  /* 1u 0.5u*/
}
.gs-group-messages-topic-digest-daily-topic ul, 
.gs-group-messages-topic-digest-daily-topic li {
    margin: 0 0 0 0;
    padding: 0 0 0 0;
}
.gs-group-messages-topic-digest-daily-topic ul {
    margin-left: 0; /* Needed */
}
.gs-group-messages-topic-digest-daily-topic li {
    list-style: none;
}
.gs-group-messages-topic-digest-daily-topic .topic {
    color: #404d40;
    font-weight: 900;
    font-size: 20px;  /* 1u */
    line-height: 30px /* 1.5u */
}
#gs-group-messages-topic-digest-daily-grouplinks-more {
    padding: 0 0 0 0;
}
#gs-group-messages-topic-digest-daily-grouplinks-new {
    padding: 0 0 0 40px;
}
</tal:block>